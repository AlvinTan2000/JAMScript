{
  "package" : {
    "name": "JView",
    "version": "1.0.0",
    "description": "A Simple JView system using React+Mobx+Node+MongoDB",
    "author": "Carl Liu"
  },
  "path" : "./app",
  "port" : "3000",
  "maxMemorySize" : "30",
  "pages" : [
    {
      "name" : "Dashboard",
      "route" : "/dashboard",
      "layout" : "default",
      "panels" : [
        {
          "type": "stacked-graph",
          "windowSize":"20",
          "store": {
            "socket":{
              "id": "0",
              "x" : "x",
              "y" : "y"
            }
          }
        },
        {
          "type": "scatter",
          "windowSize":"20",
          "store": {
            "socket":{
              "id": "0",
              "x" : "x",
              "y" : "y"
            }
          }
        }
      ]
    },
    {
      "name" : "Controller Panel",
      "route" : "/controller-panel",
      "layout" : "default",
      "panels" : [
        {
          "type": "Controller",
          "controlList": [
            {
              "id": "5",
              "type": "terminal",
              "dispLabel": "terminal",
              "commandsList": "commands",
              "className": "btn-primary",
              "buttonTrigger": "addCommand",
              "trigger": "emitValue",
              "buttonLabel": "Send"
            }
          ],
          "store": {
            "socket": {
              "id": 0
            },
            "commands": []
          }
        } 
      ]
    }
  ]
}