{
    "name": "jamscript",
    "description": "Compiler for the JAMScript language",
    "version": "0.0.1",
    "license": "MIT",
    "keywords": [
    	"c",
    	"javascript",
    	"cloud-computing",
    	"iot-framework",
    	"iot",
    	"programming-language",
    	"iot-middleware",
    	"fog-computing"
    ],
    "homepage": "https://citelab.github.io/JAMScript/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/citelab/JAMScript.git"
    },
    "bugs": "https://github.com/citelab/JAMScript/issues",
    "scripts": {
        "preinstall": "scripts/install/depend.sh",
        "install": "node-gyp rebuild",
        "postinstall": "scripts/install/runtime.sh"
    },
    "bin": { "jamc" : "./index.js" },
    "dependencies": {
        "chance": "^1.0.16",
        "jszip": "^3.1.5",
        "ohm-js": "^0.13.1",
        "random-js": "^1.0.8",
        "vorpal": "^1.12.0"
    },
    "engines": {
        "node": ">= 7.0.0"
    },
    "os": [
        "darwin",
        "linux"
    ],
    "gypfile": true,
    "preferGlobal": true,
    "files": [
        "deps",
        "lib/jamlib",
        "lib/ohm",
        "scripts/install/depend.sh",
        "scripts/install/runtime.sh",
        "tools",
        "index.js",
        "binding.gyp"
    ]
}
